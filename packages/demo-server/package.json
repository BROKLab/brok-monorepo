{
  "name": "@brok/demo-server",
  "version": "0.7.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "pnpm prisma generate && pnpm prisma migrate reset -f && nest build",
    "init:db": "pnpm prisma migrate dev --name init",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "redis": "redis-server 2>&1",
    "start:old": "nest start",
    "start": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "pnpm run redis & nest start --debug --watch",
    "start:prod": "node ./dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm run redis & jest",
    "test:watch": "pnpm run redis & jest --watch",
    "test:cov": "pnpm run redis & jest --coverage",
    "test:debug": "pnpm run redis & node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@brok/sdk": "^0.10.0",
    "@nestjs/bull": "^0.5.3",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "^3.12.0",
    "bull": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "neverthrow": "^4.3.1",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@ceramicnetwork/cli": "^1.11.1",
    "@ceramicnetwork/common": "^1.11.1-rc.0",
    "@ceramicnetwork/core": "^1.13.2",
    "@ceramicnetwork/stream-tile-handler": "^1.5.11",
    "@ethersproject/wallet": "^5.6.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bull": "^3.15.8",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethers": "^5.6.2",
    "jest": "^27.2.5",
    "node-fetch": "*",
    "prettier": "^2.3.2",
    "prisma": "^3.12.0",
    "redis": "4.0.4",
    "redis-server": "1.2.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "tslib": "^2.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}