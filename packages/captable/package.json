{
  "name": "@brok/captable",
  "description": "Contract artifacts for brok captables",
  "version": "0.5.0",
  "main": "./lib/src/index.js",
  "typings": "./lib/src/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "private": false,
  "repository": {
    "url": "https://github.com/BROKLab/brok-monorepo"
  },
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "build": "pnpm run typechain && pnpm run deploy:brokDev && pnpm run deploy:brokStage && pnpm run deploy:brokLocal && pnpm run deploy:brokProd && pnpm run compile",
    "build:local": "pnpm run typechain && pnpm run deploy:brokLocal && pnpm run compile",
    "clean": "rm -rf lib",
    "compile": "pnpm run clean && pnpx tsc -p tsconfig.lib.json",
    "test": "pnpx hardhat test",
    "typechain": "hardhat typechain",
    "deploy:brokLocal": "pnpx hardhat deploy --export src/DeploymentsBrokLocal.json",
    "deploy:brokDev": "pnpx hardhat deploy --network brokDev --export src/DeploymentsBrokDev.json",
    "deploy:brokStage": "pnpx hardhat deploy --network brokStage --export src/DeploymentsBrokStage.json",
    "deploy:brokProd": "pnpx hardhat deploy --network brokProd --export src/DeploymentsBrokProd.json",
    "start": "pnpx run-p start:contracts start:lib ",
    "start:contracts": "pnpm run deploy:brokLocal && pnpx hardhat node --watch --export src/DeploymentsBrokLocal.json",
    "start:lib": "pnpx tsc -w -p tsconfig.lib.json",
    "demo": "pnpx hardhat run scripts/createCapTable.ts --network localhost"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.0.0",
    "@ethersproject/address": "^5.6.0",
    "@ethersproject/bytes": "^5.0.0",
    "@ethersproject/hardware-wallets": "^5.0.14",
    "@ethersproject/providers": "^5.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.2.0",
    "@openzeppelin/hardhat-upgrades": "^1.16.1",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^12.20.47",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.6",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.2",
    "hardhat": "^2.9.2",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-deploy": "^0.11.4",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.8",
    "levelup": "^5.1.1",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.7.20",
    "ts-node": "^10.7.0",
    "typechain": "^7.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "dids": "^2.4.3",
    "key-did-provider-ed25519": "^1.1.0",
    "key-did-resolver": "^1.4.4"
  }
}