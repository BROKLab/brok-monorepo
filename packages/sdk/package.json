{
	"name": "@brok/sdk",
	"version": "0.12.0",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"BRÃ˜K",
		"Symfoni",
		"identity",
		"captable"
	],
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.min.mjs",
	"unpkg": "dist/index.umd.min.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"prepublishOnly": "pnpm build",
		"clean": "rm -rf dist",
		"build": "pnpm run clean && pnpm run lint && tsc --project tsconfig.json && rollup --config rollup.config.js && pnpm run build:stats",
		"watch:rollup": "rollup --config rollup.config.js --watch",
		"watch:ts": "pnpm tsc --watch",
		"watch": "pnpm run-p watch:rollup watch:ts",
		"build:stats": "(echo '\\033[35;3m' ; cd dist && ls -lh index*js index*gz | tail -n +2 | awk '{print $5,$9}')",
		"lint": "pnpm eslint ./src --ext .js,.jsx,.ts,.tsx",
		"test": "pnpm ava --serial",
		"test:debug": "DEBUG=brok* pnpm ava --serial",
		"test:watch": "pnpm ava --watch --serial"
	},
	"dependencies": {
		"@brok/captable": "^0.13.0",
		"@ceramicnetwork/3id-did-resolver": "2.1.5-rc.1",
		"@ceramicnetwork/blockchain-utils-linking": "^1.5.5",
		"@ceramicnetwork/common": "2.5.1-rc.0",
		"@ceramicnetwork/core": "2.8.1-rc.0",
		"@ceramicnetwork/http-client": "2.3.4-rc.0",
		"@ceramicnetwork/stream-tile": "2.4.3-rc.0",
		"@ceramicnetwork/stream-tile-handler": "2.2.7-rc.0",
		"@ceramicnetwork/streamid": "2.3.3-rc.0",
		"cross-fetch": "^3.1.5",
		"debug": "^4.3.4",
		"did-resolver": "^4.0.0",
		"dids": "3.2.0",
		"ethers": "^5.6.9",
		"neverthrow": "^4.3.1",
		"node-fetch": "^3.2.10"
	},
	"devDependencies": {
		"@ava/typescript": "^3.0.1",
		"@opentelemetry/api": "^1.1.0",
		"@types/debug": "^4.1.7",
		"@types/node": "^18.6.5",
		"@types/tap": "^15.0.7",
		"@typescript-eslint/eslint-plugin": "^5.33.0",
		"@typescript-eslint/parser": "^5.33.0",
		"ava": "^4.3.1",
		"dotenv": "^10.0.0",
		"eslint": "^8.21.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"rollup": "^2.77.2",
		"rollup-plugin-gzip": "^3.0.1",
		"rollup-plugin-terser": "^7.0.2",
		"tap": "^16.3.0",
		"terser": "^5.14.2",
		"typescript": "^4.7.4"
	},
	"peerDependencies": {
		"@ceramicnetwork/3id-did-resolver": "2.1.5-rc.1",
		"@ceramicnetwork/blockchain-utils-linking": "^1.5.5",
		"@ceramicnetwork/common": "2.5.0-rc.1",
		"@ceramicnetwork/core": "2.8.0-rc.1",
		"@ceramicnetwork/http-client": "2.3.2-rc.1",
		"@ceramicnetwork/stream-tile": "2.4.1-rc.1",
		"@ceramicnetwork/stream-tile-handler": "2.2.5-rc.1",
		"@ceramicnetwork/streamid": "2.3.1-rc.0",
		"cross-fetch": "^3.1.5",
		"debug": "^4.3.4",
		"did-resolver": "^4.0.0",
		"dids": "3.2.0",
		"ethers": "^5.6.9",
		"neverthrow": "^4.3.1",
		"node-fetch": "^3.2.10",
		"@opentelemetry/api": ">=1.0.0 <1.2.0"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		],
		"files": [
			"tests/**/*"
		]
	}
}