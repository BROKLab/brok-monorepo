{
	"name": "@brok/sdk",
	"version": "1.0.0",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"BRÃ˜K",
		"Symfoni",
		"identity",
		"captable"
	],
	"type": "module",
	"main": "./dist/index.js",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.js"
	},
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"tsup": "tsup",
		"prepublishOnly": "pnpm build",
		"clean": "rm -rf dist",
		"build:new": "pnpm run clean && pnpm run lint && tsup && pnpm run build:stats",
		"build": "pnpm run clean && pnpm run lint && tsc --project tsconfig.json && rollup --config rollup.config.js && pnpm run build:stats",
		"watch:rollup": "rollup --config rollup.config.js --watch",
		"watch:ts": "pnpm tsc --watch",
		"watch": "pnpm run-p watch:rollup watch:ts",
		"build:stats": "(echo '\\033[35;3m' ; cd dist && ls -lh index*js index*gz | tail -n +2 | awk '{print $5,$9}')",
		"lint": "pnpm eslint ./src --ext .js,.jsx,.ts,.tsx",
		"test": "pnpm ava --serial",
		"test:debug": "DEBUG=brok* pnpm ava --serial",
		"test:watch": "pnpm ava --watch --serial",
		"test:enc": "pnpm ava tests/encumbrance.test.ts --serial",
		"test:debug:enc": "DEBUG=brok* pnpm ava tests/encumbrance.test.ts --serial",
		"test:cap": "pnpm ava tests/captable.test.ts --serial --watch"
	},
	"dependencies": {
		"@brok/captable": "workspace:*",
		"@ceramicnetwork/3id-did-resolver": "^2.9.0",
		"@ceramicnetwork/blockchain-utils-linking": "^2.8.0",
		"@ceramicnetwork/common": "^2.16.0",
		"@ceramicnetwork/core": "^2.21.0",
		"@ceramicnetwork/http-client": "^2.13.0",
		"@ceramicnetwork/stream-tile": "^2.12.0",
		"@ceramicnetwork/stream-tile-handler": "^2.11.0",
		"@ceramicnetwork/streamid": "^2.9.0",
		"cross-fetch": "^3.1.5",
		"debug": "^4.3.4",
		"did-resolver": "^4.0.1",
		"dids": "3.4.0",
		"ethers": "^5.6.9",
		"key-did-provider-ed25519": "^2.0.1",
		"key-did-resolver": "^2.3.0",
		"neverthrow": "^4.3.1",
		"node-fetch": "^3.2.10"
	},
	"devDependencies": {
		"@ava/typescript": "^3.0.1",
		"@types/debug": "^4.1.7",
		"@types/node": "^18.11.18",
		"ava": "^4.3.1",
		"dotenv": "^10.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"tsup": "^6.5.0",
		"typescript": "^4.7.4"
	},
	"peerDependencies": {
		"@brok/captable": "workspace:*",
		"@ceramicnetwork/3id-did-resolver": "^2.9.0",
		"@ceramicnetwork/blockchain-utils-linking": "^2.8.0",
		"@ceramicnetwork/common": "^2.16.0",
		"@ceramicnetwork/core": "^2.21.0",
		"@ceramicnetwork/http-client": "^2.13.0",
		"@ceramicnetwork/stream-tile": "^2.12.0",
		"@ceramicnetwork/stream-tile-handler": "^2.11.0",
		"@ceramicnetwork/streamid": "^2.9.0",
		"cross-fetch": "^3.1.5",
		"debug": "^4.3.4",
		"did-resolver": "^4.0.1",
		"dids": "3.4.0",
		"ethers": "^5.6.9",
		"key-did-provider-ed25519": "^2.0.1",
		"key-did-resolver": "^2.3.0",
		"neverthrow": "^4.3.1",
		"node-fetch": "^3.2.10"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		],
		"files": [
			"tests/**/*"
		]
	}
}