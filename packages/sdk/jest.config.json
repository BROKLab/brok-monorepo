{
  "displayName": "sdk",
  "testEnvironment": "node",
  "roots": [
    "src/__tests__/"
  ],
  "testMatch": [
    "**/?(*.)+(spec|test).[jt]s?(x)"
  ],
  "transform": {
    "^.+\\.tsx?$": "babel-jest"
  },
  "moduleFileExtensions": [
    "ts",
    "js",
    "html"
  ],
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "coverageDirectory": "../../coverage/libs/sdk",
  "testTimeout": 20000,
  "resolver": "jest-resolver-enhanced"
}